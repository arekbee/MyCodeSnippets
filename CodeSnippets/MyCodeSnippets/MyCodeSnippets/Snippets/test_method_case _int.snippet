<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>test_method_case_int</Title>
			<Shortcut>test_method_case_int</Shortcut>
			<Description>Code snippet for test method</Description>
			<Author>Arkadiusz Beer</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
      <Imports>
        <Import>
          <Namespace>NUnit.Framework</Namespace>
        </Import>
      </Imports>
      <References>
        <Reference>
          <Assembly>nunit.framework.dlldll</Assembly>
        </Reference>
      </References>
			<Declarations>
				<Literal>
					<ID>methodname</ID>
					<ToolTip>Method evaluate</ToolTip>
					<Default>MethodName</Default>
				</Literal>
			<Literal Editable="false">
            <ID>TestCase</ID>
            <Function>SimpleTypeName(global:: NUnit.Framework.TestCaseAttribute)</Function>
          </Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[$TestCase$(-1)]
[$TestCase$(-10)]
[$TestCase$(1)]
[$TestCase$(10)]
[$TestCase$(0)
[$TestCase$(int.MaxValue)]
[$TestCase$(int.MinValue)]]			
public void  It_should_$methodname$(int arg1) 
{
	   $selected$$end$
}]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>